			public function set developmentProjectAB(value:DesarrolloProyecto):void{
				if( value == null ){
					return;
				}
				this.cleanForm();
				this._developmentProjectAB = value;
				this.setDevelopmentProjectAB(); 
			}
			
			private function setDevelopmentProjectAB():void
