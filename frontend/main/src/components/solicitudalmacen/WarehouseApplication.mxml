				this.cmbApplicantArea.selectedIndex = 0;
				this.txtSite.reset();
				this.cmbAuthorizedBy.selectedIndex = 0;
				//this.autCmpDescription.reset();
				this.txtQuantity.reset();
				this.txtObservation.reset();
			}
			
